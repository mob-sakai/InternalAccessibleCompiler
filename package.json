{
  "name": "internal-accessible-compiler",
  "version": "0.0.0",
  "scripts": {
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.4.0-beta.9",
    "semantic-release": "^16.0.0-beta.46"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release/github",{
        "assets": [
          {"path": "publish/**"}
        ]
      }],
      ["@semantic-release/exec", {
        "prepareCmd": "./prepare.sh ${nextRelease.version}",
        "publishCmd": "dotnet nuget push bin/Release/*.nupkg -k $NUGET_TOKEN -s https://api.nuget.org/v3/index.json"
      }]
    ]
  }
}
